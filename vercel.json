{
  "version": 2,
  "builds": [
    {
      "src": "apps/business-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "cd apps/business-app && pnpm run build"
}
